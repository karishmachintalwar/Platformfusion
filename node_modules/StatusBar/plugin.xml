<?xml version='1.0' encoding='utf-8'?>
<plugin id="com.tangide.statusbar" version="1.0.0" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
	<name>StatusBar</name>

   <license>Apache 2.0 License</license>
   <description>marry plugin description</description>
   <keywords>marry</keywords>


	<js-module name="StatusBar" src="www/StatusBar.js">
		<clobbers target="cordova.plugins.StatusBar" />
	</js-module>
	<platform name="android">
	<config-file parent="/*" target="res/xml/config.xml">
		<feature name="StatusBar">
			<param name="android-package" value="com.tangide.statusbar.StatusBar" />
		</feature>
	</config-file>
	<config-file parent="/*" target="AndroidManifest.xml">
	</config-file>
	<source-file src="src/android/StatusBar.java" target-dir="src/com/tangide/statusbar/StatusBar" />
</platform>
</plugin>